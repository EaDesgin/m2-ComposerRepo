<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-packages-ticket-link" after="-">
                <arguments>
                    <argument name="path" xsi:type="string">eadesign_composerrepo/index/packages</argument>
                    <argument name="label" xsi:type="string">Packages</argument>
                    <argument name="sortOrder" xsi:type="number">1900</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-auth-ticket-link" after="-">
                <arguments>
                    <argument name="path" xsi:type="string">eadesign_composerrepo/index/auth</argument>
                    <argument name="label" xsi:type="string">Auth Keys</argument>
                    <argument name="sortOrder" xsi:type="number">1800</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\Delimiter" name="customer-account-navigation-delimiter-10" template="Magento_Customer::account/navigation-delimiter.phtml">
                <arguments>
                    <argument name="sortOrder" xsi:type="number">2</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
